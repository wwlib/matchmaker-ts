{
  "name": "matchmaker-ts",
  "version": "0.1.0",
  "description": "A game matchmaking example using TypeScript (node).",
  "main": "dist/main/index.ts",
  "scripts": {
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
